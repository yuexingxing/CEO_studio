<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/root_layout_ll_gray">

    <RelativeLayout
        android:id="@+id/rl_mess_name"
        android:clickable="true"
        android:background="@drawable/slt_layout_rect_gray"
        android:layout_width="match_parent"
        android:layout_height="@dimen/com_field_height_44"
        android:layout_marginTop="@dimen/sub_margin_top_12"
        android:gravity="center_vertical">
        <TextView
            style="@style/text_16_black_1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin_16"
            android:text="@string/msg_mess_name" />
        <TextView
            android:id="@+id/tv_mess_name"
            style="@style/text_14_black_3"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/field_txt_margin_right_36"
            android:text="@string/msg_default_mess_name" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/activity_mess_info_padding_16"
            android:contentDescription="@null"
            android:src="@mipmap/icon_jiantou_r" />
    </RelativeLayout>

    <View style="@style/driver_h" />

    <RelativeLayout
        android:id="@+id/rl_mess_status"
        android:clickable="true"
        android:background="@drawable/slt_layout_rect_gray"
        android:layout_width="match_parent"
        android:layout_height="@dimen/com_field_height_44"
        android:layout_marginTop="@dimen/sub_margin_top_12"
        android:gravity="center_vertical">


        <TextView
            style="@style/text_16_black_1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin_16"
            android:text="@string/msg_business_state" />

        <TextView
            android:id="@+id/tv_mess_status"
            style="@style/text_14_black_3"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/field_txt_margin_right_36"
            android:text="@string/msg_under_business"
            android:textColor="@color/green" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/activity_horizontal_margin_16"
            android:contentDescription="@null"
            android:src="@mipmap/icon_jiantou_r" />
    </RelativeLayout>

    <View
        style="@style/driver_h"
        android:layout_marginLeft="@dimen/activity_horizontal_margin_16"
        android:layout_marginRight="@dimen/activity_horizontal_margin_16" />


    <RelativeLayout
        android:id="@+id/rl_deliver_scope"
        android:clickable="true"
        android:background="@drawable/slt_layout_rect_gray"
        android:layout_width="match_parent"
        android:layout_height="@dimen/com_field_height_44"
        android:gravity="center_vertical">


        <TextView
            style="@style/text_16_black_1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin_16"
            android:text="@string/msg_business_deliver_scope" />

        <TextView
            style="@style/text_14_black_3"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/field_txt_margin_right_36"
            android:text="@string/msg_business_deliver_scope" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/activity_horizontal_margin_16"
            android:contentDescription="@null"
            android:src="@mipmap/icon_jiantou_r" />
    </RelativeLayout>

    <RelativeLayout
        android:visibility="gone"
        android:id="@+id/rl_mess_notice"
        android:clickable="true"
        android:background="@drawable/slt_layout_rect_gray"
        android:layout_width="match_parent"
        android:layout_height="@dimen/com_field_height_44"
        android:gravity="center_vertical">


        <TextView
            style="@style/text_16_black_1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin_16"
            android:text="@string/msg_mess_notice" />

        <TextView
            android:id="@+id/tv_mess_notice"
            style="@style/text_14_black_3"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/field_txt_margin_right_36"
            android:singleLine="true"
            android:maxEms="10"
            android:ellipsize="end"
            android:text="@string/msg_mess_notice" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/activity_horizontal_margin_16"
            android:contentDescription="@null"
            android:src="@mipmap/icon_jiantou_r" />
    </RelativeLayout>

